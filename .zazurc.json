{
  "hotkey": "alt+space",
  "theme": "iansinnott/zazu-dark-theme",
  "displayOn": "detect",
  "plugins": [
    "tinytacoteam/zazu-calculator",
    "tinytacoteam/zazu-template",
    "tinytacoteam/zazu-system",
    "tinytacoteam/zazu-package-manager",
    {
      "name": "tinytacoteam/zazu-file-finder",
      "variables": {
        "append": false,
        "directories": {
          "appPath": [
            "/Applications",
            "/System/Applications"
          ],
          "filePath": [
            "~"
          ],
          "excludeName": [
            "node_modules"
          ],
          "excludePath": [
            "/System"
          ]
        }
      }
    }
  ]
}
