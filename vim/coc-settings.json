{
  "languageserver": {
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": [
        "dockerfile"
      ],
      "args": [
        "--stdio"
      ]
    },
    "terraform": {
      "command": "terraform-ls",
      "args": [
        "serve"
      ],
      "filetypes": [
        "terraform",
        "tf"
      ],
      "initializationOptions": {}
    }
  },
  "coc.preferences.formatOnSaveFiletypes": [
    "go",
    "javascript",
    "json",
    "typescript",
    "yaml"
  ]
}
